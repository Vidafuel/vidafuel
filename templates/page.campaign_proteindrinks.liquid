<script>
  $(document).ready(function() {

    setTimeout(function() {
      AOS.init({
        offset: -200
      });
    }, 200);

    var root = $('html,body');
	
    $(".my-product-form").change(function(e){
      var els = $(this).parent().parent().find("select.product-form__variants option");
      for(var i=0;i<els.length;i++){
       	if($.trim($(els.get(i)).text()) == $.trim($(this).val()))
        {
    	  $($(this).parent().parent().find("select.product-form__variants")).val($(els.get(i)).val());
          $($(this).parent().parent().find(".bold-ro__one-time-price")).text($(els.get(i)).data("price"));
          $($(this).parent().parent().find(".price-number")).text($(els.get(i)).data("price"));
        }
      }

    });
    $(".qty-btn.left").click(function(e){
      var elInput = $($(this).parent().find("input"));
      if(parseInt(elInput.val()) >=2 )
        elInput.val(parseInt(elInput.val())-1);
    });
    
    $(".qty-btn.right").click(function(e){
      var elInput = $($(this).parent().find("input"));
      elInput.val(parseInt(elInput.val())+1);
    });
    
   $(".video-1 video").get(0).onended = function() {
        $(".video-image-1").removeClass("hidden");
        $(".video-1 video").addClass("hidden");
    };

    $(".video-2 video").get(0).onended = function() {
        $(".video-image-2").removeClass("hidden");
        $(".video-2 video").addClass("hidden");
    };

 $(".video-image-1").click(function(e)
    {
        $(".video-image-1").addClass("hidden");
        $(".video-1 video").removeClass("hidden");
        $(".video-1 video").get(0).play();
        
    });

    $(".video-image-2").click(function(e)
    {
        $(".video-image-2").addClass("hidden");
        $(".video-2 video").removeClass("hidden");
        $(".video-2 video").get(0).play();
    });

    function smoothScrollingTo(target) {
      var target = $(target);
      if (target.length) {
        root.animate({
          scrollTop: target.offset().top
        }, 600, "swing");
      }
    }
  });
</script>