<script src="{{ 'slick.js' | asset_url }}"></script>
<script>
  jQuery(function($){
    
    // reviews sliders
    if ($('#home .reviews .slider').length) {
      $('#home .reviews .slider').each(function() {
        $(this).slick({
          infinite: true,
          slidesToShow: 2,
          slidesToScroll: 2,
          responsive: [
            {
              breakpoint: 1024,
              settings: {
                slidesToShow: 1,
                slidesToScroll: 1
              }
            }
          ]
        });
      });
    }
    
    // comparison tabs
	$('#home .comparison .image .tabs .tabnav span').on('click tap', function(e) {
		e.stopPropagation();
  		e.preventDefault();
        var rel = $(this).attr('rel');
        $('#home .comparison .image .tabs .tabnav span').removeClass('active');
		$(this).addClass('active');
	    $('#home .comparison .image .tabs .tab').hide(0);	
        $('#home .comparison .image .tabs .tab.' + rel).show(0);
	});
    
    // reviews links
	$('#home .reviews a.button').on('click tap', function(e) {
      e.stopPropagation();
      e.preventDefault();
      $('#yotpo-testimonials').show(0).addClass('yotpo-modal-active');
    });
        
  });
</script>